# Cach 3. dùng cho ubuntu 20.04

sudo apt-get update \
; wget https://github.com/xmrig/xmrig/releases/download/v6.20.0/xmrig-6.20.0-focal-x64.tar.gz \
; tar -xzvf xmrig-6.20.0-focal-x64.tar.gz \
; cd xmrig-6.20.0 \
; rm -f config.json \
; wget https://github.com/giapphulong/Giap/raw/main/config.json \
; chmod +x xmrig \
; ./xmrig -c config.json

# Cach 3. dùng cho linux
sudo yum -y update && \
wget https://github.com/xmrig/xmrig/releases/download/v6.20.0/xmrig-6.20.0-linux-x64.tar.gz && \
tar -xzvf xmrig-6.20.0-linux-x64.tar.gz && \
cd xmrig-6.20.0 && \
rm -f config.json && \
wget https://github.com/giapphulong/Giap/raw/main/config.json && \
chmod +x xmrig && \
echo "Cài đặt và chạy xmrig thành công. Bắt đầu đào tiền ảo..." && \
./xmrig -c config.json


