sudo su
screen -R


# Cập nhật danh sách gói
sudo apt-get update

# Tải xuống gói XMrig từ trang GitHub
wget https://github.com/xmrig/xmrig/releases/download/v6.20.0/xmrig-6.20.0-focal-x64.tar.gz

# Giải nén gói XMrig
tar -xzvf xmrig-6.20.0-focal-x64.tar.gz

# Di chuyển đến thư mục vừa giải nén
cd xmrig-6.20.0

# Xóa file config.json nếu có
rm -f config.json

# Tải xuống file config.json mới từ GitHub
wget https://github.com/giapphulong/Giap/raw/main/config.json

# Cấp quyền thực thi cho tệp thực thi xmrig
chmod +x xmrig

# Chạy XMrig với cấu hình từ file config.json
./xmrig -c config.json



